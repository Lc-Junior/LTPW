    <body>
        <h1>Simulador de Investimento</h1>
        <form action="simulacao.do" method="post">
            <div class="form-group">
                <input class="form-control" type="text" placeholder="Digite o Valor" name="valor">
                <input class="form-control" type="text" placeholder="Digite o valor do juros anual" name="juros">
                <input class="form-control" type="text" placeholder="Digite a quantidade de meses" name="meses">
               <button type="submit" class="btn btn-primary">Simular</button>
            </div>
        </form>     
    </body>
